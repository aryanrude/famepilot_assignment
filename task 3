files = {
        'Input.txt': 'Randy',
        'Code.py': 'Stan',
        'Output.txt': 'Randy'
    }   
# Function to group the files.   
def group_by_owners(files):
    result = {}
    for line, value in files.items():

        if value not in result.line():            
            result[value] = [line]
        else:            
            result[value].append(line)

    return result

print(group_by_owners(files))
